<Window
    x:Class="VentureForge.ExamineExistingAssets"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:VentureForge"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="ExamineExistingAssets"
    Width="800"
    Height="450"
    Background="#04c582"
    mc:Ignorable="d">
    
    <Grid>
        <Button
            x:Name="SignIn"
            Margin="705,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="SignIn_Click"
            Content="Sign in" />
        <Button
            x:Name="Login"
            Margin="750,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="Login_Click"
            Content="Login" />
        <Button
            x:Name="Home"
            Margin="10,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="Home_Click"
            Content="Home" />
        <Button
            x:Name="RulesDocument"
            Width="210"
            Height="40"
            Margin="309,89,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="RulesDocument_Click"
            Content="Rules" />
        <Button
            x:Name="PreMadeCharacters"
            Width="210"
            Height="50"
            Margin="309,206,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="PreMadeCharacters_Click"
            Content="Characters" />
        <Button
            x:Name="WeaponList"
            Width="210"
            Height="45"
            Margin="309,147,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="WeaponList_Click"
            Content="Weapons" />
        <Button
            x:Name="PreviousPage"
            Margin="68,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="PreviousPage_Click"
            Content="previous" />
    </Grid>
</Window>