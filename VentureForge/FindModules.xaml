<Window
    x:Class="VentureForge.FindModules"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:VentureForge"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="FindModules"
    Width="800"
    Height="450"
    Background="#04c582"
    mc:Ignorable="d">

    <Border
        Margin="20"
        Background="#2e3137"
        CornerRadius="20">
        <StackPanel>
            <Grid>
                <Label
                    HorizontalAlignment="Center"
                    Content="Find Modules"
                    FontFamily="Georgia"
                    FontSize="35"
                    Foreground="White" />

                <Button
                    Width="75"
                    Margin="0,70,0,0"
                    HorizontalAlignment="Left"
                    Background="#545d6a"
                    Click="SignIn_Click"
                    Content="Home"
                    FontFamily="Georgia"
                    FontSize="14"
                    Foreground="White"
                    RenderTransformOrigin="-0.331,-0.006" />

                <StackPanel x:Name="sp" Height="100" />
            </Grid>
        </StackPanel>
    </Border>
</Window>
