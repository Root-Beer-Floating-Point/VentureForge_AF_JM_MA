<Window
    x:Class="VentureForge.CreateTable"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:VentureForge"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="CreateTable"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <Button
            Width="75"
            Margin="372,170,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="CreateAsset_Click"
            Content="Create Asset" />
        <Button
            Width="75"
            Margin="372,194,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="AddAsset_Click"
            Content="Add Asset" />
        <Button
            Width="75"
            Margin="31,40,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="Home_Click"
            Content="Home" />
        <Button
            Width="75"
            Margin="688,40,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="Back_Click"
            Content="Back"
            RenderTransformOrigin="0.553,-0.896" />

    </Grid>
</Window>
