<Window x:Class="VentureForge.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VentureForge"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Create a New Module" HorizontalAlignment="Left" Margin="391,196,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.114,-0.51" Click="CreateModule_Click"/>
        <Button Content="Find Existing Modules" HorizontalAlignment="Left" Margin="391,263,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.114,-0.51" Click="FindModules_Click"/>
        <Button Content="Saved and Created Modules" HorizontalAlignment="Left" Margin="391,93,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.114,-0.51" Click="Modules_Click"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="417,141,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.114,-0.51" Click="Login_Click"/>
    </Grid>
</Window>
